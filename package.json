    "postcss": "^8.2.13",
    "prettier": "^2.2.1",
    "react": "^17.0.2",
    "react-docgen-typescript": "^1.22.0",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-styleguidist": "^11.1.6",
    "rimraf": "^3.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
